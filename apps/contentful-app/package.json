{
  "name": "contentful-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@contentful/app-sdk": "4.13.0",
    "@contentful/f36-components": "4.21.2",
    "@contentful/f36-tokens": "4.0.1",
    "@contentful/react-apps-toolkit": "1.2.8",
    "contentful-management": "10.19.0",
    "emotion": "10.0.27",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.1",
    "ui": "workspace:*"
  },
  "scripts": {
    "dev": "cross-env BROWSER=none PORT=3000 react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-app-definition": "contentful-app-scripts create-app-definition",
    "upload": "contentful-app-scripts upload --bundle-dir ./build",
    "upload-ci": "contentful-app-scripts upload --ci --bundle-dir ./build  --organization-id $CONTENTFUL_ORG_ID --definition-id $CONTENTFUL_APP_DEF_ID --token $CONTENTFUL_ACCESS_TOKEN"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@contentful/app-scripts": "1.1.34",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "12.1.5",
    "@tsconfig/create-react-app": "1.0.2",
    "@types/jest": "29.2.0",
    "@types/node": "18.11.7",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "autoprefixer": "^10.4.13",
    "cross-env": "7.0.3",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.18",
    "tailwind-config": "*",
    "tailwindcss": "^3.2.1",
    "tsconfig": "workspace:*",
    "typescript": "4.8.4"
  },
  "homepage": "."
}
